<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXPasswordField?>
<?import com.jfoenix.controls.JFXTextField?>
<?import java.net.URL?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cr.ac.una.evacomuna.controller.LoginViewController">
    <stylesheets>
        <URL value="@styles.css" />
    </stylesheets>
   <VBox layoutX="50.0" layoutY="50.0">
      <children>
      
                <!-- Add your login components here -->
          <JFXTextField fx:id="txtUsuario" promptText="Username" />
          <JFXPasswordField fx:id="txtClave" promptText="Password" />
         <HBox>
            <children>
                <JFXButton fx:id="btnLogin" onAction="#onActionBtnLogin" text="Login" HBox.hgrow="ALWAYS" />
                <JFXButton fx:id="btnCancelar" onAction="#onActionBtnCancelar" text="Cancel" HBox.hgrow="ALWAYS" />
               <JFXButton fx:id="btnOlvidarClave" onAction="#onActionBtnOlvidarClave" text="Olvidar Clave" HBox.hgrow="ALWAYS" />
            </children>
         </HBox>
      </children>
   </VBox>

</AnchorPane>
